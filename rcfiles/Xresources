*.termName: screen-256color

!
! Dont forget to link URxvt-perls to $HOME/.urxvt/ext
!
! URxvt
! General settings
URxvt.saveLines: 1000
URxvt.scrollBar: False
URxvt.scrollstyle: rxvt
URxvt.urgentOnBell: True
URxvt.cursorBlink: True
URxvt.cursorColor: #dd00cc
URxvt.font: xft:Monospace-9
URxvt.letterSpace: -1
URxvt.depth: 32
URxvt.transparent: False
! URxvt.shading: 20


! Extensions
URxvt.perl-ext-common: default,matcher,url-select
URxvt.url-launcher: xdg-open
URxvt.matcher.launcher: xdg-open
URxvt.matcher.button: 1

URxvt.copyCommand:  xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard

! Keybindings
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.M-u: perl:url-select:select_next

! URxvt.keysym.Mod4-C-v: perl:clipboard:paste_escaped
! URxvt.keysym.M-c: perl:clipboard:copy
! URxvt.keysym.M-v: perl:clipboard:paste
! URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
! URxvt.keysym.M-Escape: perl:keyboard-select:activate
! URxvt.keysym.C-Escape: perl:keyboard-select:activate
! URxvt.keysym.M-u: perl:url-select:select_next



! Fuzzy launcher
URxvtFuzzy*internalBorder: 5
URxvtFuzzy*background: #1d1f21
URxvtFuzzy.transparent: False
URxvtFuzzy.shading: 0
